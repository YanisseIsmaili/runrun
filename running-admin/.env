# Fichier .env pour le frontend (running-admin)
# URL de l'API - Mode écoute réseau
# running-admin/.env
VITE_API_BASE_URL=http://localhost:5000

# Environment (development, production, test)
VITE_APP_ENV=development

# Configuration du debugging
VITE_DEBUG_MODE=true

# Configuration de sécurité
VITE_ENABLE_CRYPTO=true

# Configuration réseau
VITE_NETWORK_TIMEOUT=30000
VITE_RETRY_ATTEMPTS=3

# Cache des statistiques (en secondes)
VITE_STATS_CACHE_DURATION=1
