# Fichier .env pour le frontend (running-admin)
# URL de l'API - Mode écoute réseau
# running-admin/.env
VITE_API_BASE_URL=http://localhost:5000

# Configuration des APIs par défaut - Format JSON
REACT_APP_DEFAULT_APIS='[
  {"ip": "192.168.0.47", "name": "Dev Server"},
  {"ip": "127.0.0.1", "name": "Loopback"},
  {"ip": "localhost", "name": "Serveur Principal"},
  {"ip": "192.168.27.66", "name": "Serveur Distant"},
  {"ip": "192.168.0.1", "name": "Gateway"},
  {"ip": "10.0.0.1", "name": "VPN Gateway"},
  {"ip": "192.168.27.77", "name": "API Yaniss"}
]'

# Environment (development, production, test)
VITE_APP_ENV=development

# Configuration du debugging
VITE_DEBUG_MODE=true

# Configuration de sécurité
VITE_ENABLE_CRYPTO=true

# Configuration réseau
VITE_NETWORK_TIMEOUT=30000
VITE_RETRY_ATTEMPTS=3

# Cache des statistiques (en secondes)
VITE_STATS_CACHE_DURATION=1