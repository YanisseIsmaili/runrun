{
  "expo": {
    "name": "Running App",
    "slug": "running-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4CAF50"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#4CAF50"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "newArchEnabled": false,
    "sdkVersion": "53.0.0",
    "jsEngine": "jsc",
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permettez à l'application de suivre votre position pour enregistrer vos courses."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "L'application a besoin d'accéder à vos photos pour mettre à jour votre photo de profil."
        }
      ]
    ]
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "axios": "^1.8.4",
    "expo": "~53.0.0",
    "expo-image-picker": "~14.7.1",
    "expo-location": "~16.5.2",
    "expo-status-bar": "~1.11.1",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-maps": "1.10.0",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "4.9.0",
    "react-native-screens": "~3.29.0",
    "setimmediate": "^1.0.5"
  },
  "devDependencies": {
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "react-native-dotenv": "^3.4.11"
  }
}
